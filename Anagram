//Check Anagram
//Given two strings s1 and s2, check if they are anagrams (same characters, same frequency).
//Example:
//Input: "listen", "silent" ? Output: YES

#include <iostream>
#include <string>
using namespace std;

bool areAnagrams(string s1, string s2) {
    if (s1.size() != s2.size()) return false;

    int freq[256] = {0};  // ASCII character count

    // Count characters in s1
    for (int i = 0; i < s1.size(); i++) {
        freq[(unsigned char)s1[i]]++;
    }

    // Subtract counts using s2
    for (int i = 0; i < s2.size(); i++) {
        freq[(unsigned char)s2[i]]--;
    }

    // Check if all frequencies are zero
    for (int i = 0; i < 256; i++) {
        if (freq[i] != 0) return false;
    }
    return true;
}

int main() {
    string s1, s2;
    cin >> s1 >> s2;   // enter two words (e.g., listen silent)

    if (areAnagrams(s1, s2))
        cout << "YES" << endl;
    else
        cout << "NO" << endl;

    return 0;
}
